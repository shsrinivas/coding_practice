#include <stdio.h>

int itoa(int num, char str[]);
{
	for ( int i = 0 ; str[i] != '\0' ; i++ )
	{
		str[i] = num / 100;
	}
}

int main()
{
    int num;
    char str[10];
    
    printf("Enter the number:");
    scanf("%d", &num);
    
    iota(num, str);
    
    printf("Integer to string is %s", str);
}
